    <dependencies>
        <haxelib name="nme" version="3.4.0" />
        <haxelib name="polygonal-ds" version="1.36" />
        <haxelib name="polygonal-ui" version="1.00" />
        <haxelib name="msignal" version="1.0.0" />
    </dependencies>
</project>
