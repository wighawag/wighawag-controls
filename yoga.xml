<?xml version="1.0" encoding="utf-8" ?>
<project>
    <yoga-version>1</yoga-version>
    <compiler-version>2.10</compiler-version>

    <id>com.wighawag.controls</id>
    <version>0.1.1-SNAPSHOT</version>

    <main>Main</main>

    <sources>
        <source path="src"/>
    </sources>

    <tests path="test" framework="munit"/>

    <targets hxml="compile">
        <target name="swf">
            <param line="-swf-version 11"/>
        </target>
	<target name="cpp"/>
	<target name="js"/>
    </targets>
    
    <dependencies>
        <haxelib name="nme"/>
        <haxelib name="polygonal-ds" version="1.36"/>
        <haxelib name="polygonal-ui" version="1.00"/>
        <haxelib name="msignal" version="1.0.0"/>
    </dependencies>
</project>
